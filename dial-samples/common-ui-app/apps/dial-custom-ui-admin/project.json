{
  "name": "dial-custom-ui-admin",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/dial-custom-ui-admin",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/dial-custom-ui-admin",
        "assets": []
      },
      "configurations": {
        "development": {
          "outputPath": "apps/dial-custom-ui-admin"
        },
        "production": {
          "generatePackageJson": true
        }
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "dial-custom-ui-admin:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "dial-custom-ui-admin:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "dial-custom-ui-admin:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "dial-custom-ui-admin:build:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/dial-custom-ui-admin/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ]
    }
  },
  "tags": [
    "type:app"
  ]
}
